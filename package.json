{
  "name": "twitter-ai-automation",
  "version": "1.0.0",
  "description": "Twitter AI Automation tool for scraping tweets and posting AI-generated content",
  "scripts": {
    "backend": "cd backend && npm run start",
    "frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "postinstall": "cd backend && npm install && cd ../frontend && npm install",
    "setup": "npm install && npm run postinstall"
  },
  "keywords": [
    "twitter",
    "x",
    "ai",
    "openai",
    "automation",
    "tweet-capture"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
