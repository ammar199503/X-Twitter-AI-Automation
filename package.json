{
  "name": "twitter-scraper-and-poster",
  "version": "1.0.0",
  "description": "A tool for automatically capturing and reposting tweets with screenshots",
  "scripts": {
    "backend": "cd backend && npm run start",
    "frontend": "cd frontend && npm start",
